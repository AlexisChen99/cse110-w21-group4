{
  "name": "cse110-w21-group4",
  "version": "1.0.0",
  "description": "<a href=\"https://www.youtube.com/watch?v=Gd6j-zVnezI&feature=youtu.be\" target=\"_blank\">Intro Video</a>",
  "main": "index.js",
  "jest": {
    "testRegex": "source/tests/.*\\.(js|jsx)$"
  },
  "scripts": {
    "eslint": "eslint -c .eslintrc.json --ignore-path .lintgitignore ./source --fix",
    "test": "jest --coverage",
    "gendocs": "jsdoc -d ./docs ./source",
    "cypress:open": "cypress open",
    "build": "npm run instrument && npm run cp",
    "preinstrument": "npm run clean",
    "instrument": "nyc instrument --compact false source build/source",
    "cp": "mkdir build/source/img && mkdir build/source/audio && cp source/*.css build/source && cp source/img/*.png build/source/img/ && cp source/potato.html build/source && cp source/audio/* build/source/audio/",
    "clean": "rm -rf build .nyc_output || true",
    "report:coverage": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexisChen99/cse110-w21-group4.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexisChen99/cse110-w21-group4/issues"
  },
  "homepage": "https://github.com/AlexisChen99/cse110-w21-group4#readme",
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.2",
    "cypress": "^6.6.0",
    "cypress-localstorage-commands": "^1.4.1",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6"
  }
}
